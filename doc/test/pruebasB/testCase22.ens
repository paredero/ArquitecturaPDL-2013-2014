; INICIO PROGRAMA PRINCIPAL pasQal 
RES 11
MOVE #65535, .SP
MOVE .SP , .IY
SUB  .SP , #15
MOVE .A , .SP
MOVE .IY , /0
NOP
; Traducir Quadruple - [ASIG A, 10, null]
MOVE #10, #-5[.IY]
; Traducir Quadruple - [ASIG B, 20, null]
MOVE #20, #-6[.IY]
; Escribimos un salto de linea
WRSTR /cadena0
WRSTR /cadena1
WRINT #30
; Escribimos un salto de linea
WRSTR /cadena0
WRSTR /cadena2
WRINT #-5[.IY]
; Escribimos un salto de linea
WRSTR /cadena0
WRSTR /cadena3
SUB #-6[.IY], #-5[.IY]
MOVE .A, #-11[.IY]
; Traducir Quadruple - [ASIG A, _T_0, null]
MOVE #-11[.IY], #-5[.IY]
WRINT #-5[.IY]
; Escribimos un salto de linea
WRSTR /cadena0
WRSTR /cadena4
SUB #100, #50
MOVE .A, #-12[.IY]
SUB #-12[.IY], #30
MOVE .A, #-13[.IY]
SUB #-13[.IY], #10
MOVE .A, #-14[.IY]
; Traducir Quadruple - [ASIG A, _T_3, null]
MOVE #-14[.IY], #-5[.IY]
WRINT #-5[.IY]
; Escribimos un salto de linea
WRSTR /cadena0
HALT 

; Inicio Cadenas de Texto

cadena0:  DATA "\n"
cadena1:  DATA "ESCRIBE NUMERO (30): "
cadena2:  DATA "VARIABLE A(10): "
cadena3:  DATA "RESTA 20 - 10= (10): "
cadena4:  DATA "RESTA 100-50-30-10 = (10): "
