; INICIO PROGRAMA PRINCIPAL pasQal 
RES 21
MOVE #65535, .SP
MOVE .SP , .IY
SUB  .SP , #8
MOVE .A , .SP
MOVE .IY , /0
;-- Definicion FUNCION/PROCEDIMIENTO 
BR /FIN_L0
L0 :

NOP
SUB #3, #7
MOVE .A, #-6[.IY]
MOVE #-6[.IY] , #0[.IY]
MOVE #L1 , .PC

WRSTR /cadena1
; Escribimos un salto de linea
WRSTR /cadena0
; Traducir Quadruple - [ASIG J, 2, null]
MOVE #2, #-5[.IY]
WRSTR /cadena2
WRINT #-5[.IY]
; Escribimos un salto de linea
WRSTR /cadena0
MOVE #100 , #0[.IY]
MOVE #L1 , .PC

L1 : 
; Retorno Argumentos REFERENCIA 
MOVE #-4[.IY] , .R5 
MOVE .R5 , .PC 
REF_L0: 
; Retorno Subprograma 
MOVE #0[.IY] , .R9
MOVE #-1[.IY] , .R7
MOVE #-2[.IY] , /0
MOVE .IY , .SP
MOVE .IX , .IY
MOVE #-3[.IY], .IX
MOVE .R7 , .PC
FIN_L0 : 

WRSTR /cadena3
; Escribimos un salto de linea
WRSTR /cadena0
; INICIO ARGUMENTOS FIN
; Llamada Funcion Quadruple - [CALL FACT, 0, _T_0]
MOVE .SP , .IX
MOVE /0 , #-2[.IY]
MOVE .IY , /0
MOVE .IY , .IX
MOVE .SP , .IY
SUB .SP , #7
MOVE .A , .SP
MOVE #RET_L2 , #-1[.IY]
MOVE .IX , #-3[.IY]
MOVE #REF_L2 , #-4[.IY]
BR /L0
REF_L2:
BR /REF_L0
RET_L2: 
MOVE .R9 , #-6[.IY]

SUB #-6[.IY], #5
MOVE .A, #-7[.IY]
; Traducir Quadruple - [ASIG A, _T_1, null]
MOVE #-7[.IY], #-5[.IY]
; Escribimos un salto de linea
WRSTR /cadena0
WRSTR /cadena4
WRINT #-5[.IY]
; Escribimos un salto de linea
WRSTR /cadena0
HALT 

; Inicio Cadenas de Texto

cadena0:  DATA "\n"
cadena1:  DATA "SENTENCIA DETRAS DE UN VALOR DEVUELTO"
cadena2:  DATA "SENTENCIA DETRAS DE VALOR DEVUELTO DEBE ESCRIBIR (2): "
cadena3:  DATA "NUEVA EJECUCION---"
cadena4:  DATA "IMPRIME VALOR DEVUELTO POR UNA FUNCION CON DOS 'DEVUELVES', EN EL PRIMERO ACABA(-9): "
