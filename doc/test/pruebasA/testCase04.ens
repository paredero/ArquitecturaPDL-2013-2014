; INICIO PROGRAMA PRINCIPAL pasQal 
RES 11
MOVE #65535, .SP
MOVE .SP , .IY
SUB  .SP , #7
MOVE .A , .SP
MOVE .IY , /0
NOP
WRSTR /cadena1
; Escribimos un salto de linea
WRSTR /cadena0
ADD #1, #1
MOVE .A, #-6[.IY]
; Traducir Quadruple - [ASIG A, _T_0, null]
MOVE #-6[.IY], #-5[.IY]
WRSTR /cadena2
WRINT #-5[.IY]
HALT 

; Inicio Cadenas de Texto

cadena0:  DATA "\n"
cadena1:  DATA "VARIABLES Y ARITMETICA SIMPLE"
cadena2:  DATA "A(2):"
